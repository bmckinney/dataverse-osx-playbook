<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC
     "-//Apple Computer//DTD PLIST 1.0//EN" "
    http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>solr</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/java</string>
        <string>-Djetty.home=/usr/local/solr-4.6.0/example</string>
        <string>-Djetty.logs=/usr/local/solr-4.6.0/example/logs</string>
        <string>-Dsolr.solr.home=/usr/local/solr-4.6.0/example/solr</string>
        <string>-jar</string>
        <string>/usr/local/solr-4.6.0/example/start.jar</string>
    </array>
    <key>ServiceDescription</key>
    <string>Apache Solr Server</string>
    <key>WorkingDirectory</key>
    <string>/usr/local/solr-4.6.0/example</string>
    <key>StandardErrorPath</key>
    <string>/usr/local/solr-4.6.0/solr-error.log</string>
    <key>StandardOutPath</key>
    <string>/usr/local/solr-4.6.0/solr-output.log</string>
    <key>OnDemand</key>
    <true/>
</dict>
</plist>